{
  "name": "@samuel-erb/strapi-plugin-lucide-icon-picker",
  "version": "1.0.2",
  "description": "A Strapi v5 custom field plugin for selecting Lucide icons. Icons are stored as kebab-case strings.",
  "keywords": [
    "strapi",
    "strapi-plugin",
    "custom-field",
    "icons",
    "lucide",
    "icon-picker",
    "ui",
    "cms"
  ],
  "type": "commonjs",
  "main": "./dist/server/index.js",
  "module": "./dist/server/index.mjs",
  "types": "./dist/server/src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuel-erb/strapi-plugin-lucide-icon-picker.git"
  },
  "homepage": "https://github.com/samuel-erb/strapi-plugin-lucide-icon-picker#readme",
  "bugs": {
    "url": "https://github.com/samuel-erb/strapi-plugin-lucide-icon-picker/issues"
  },
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/admin": "^5.23.2",
    "@strapi/design-system": "^2.0.0-rc.29",
    "@strapi/icons": "^2.0.0-rc.29",
    "lucide-react": "^0.542.0",
    "react-intl": "^7.1.11"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.23.2",
    "@strapi/typescript-utils": "^5.23.2",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.23.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "lucide-icon-picker",
    "displayName": "Lucide Icon Picker",
    "description": "A Strapi v5 custom field plugin for selecting Lucide icons. Icons are stored as kebab-case strings."
  },
  "license": "MIT",
  "author": {
    "name": "Samuel Erb",
    "email": "samuel.erb@me.com"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
